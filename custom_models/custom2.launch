<launch>
<!-- Load the custom URDF model into the parameter server -->
    <param name="robot_description" textfile="$(find custom_models)/custom2.urdf" />
    <!-- <param name="robot_description" textfile="$(find customm_models)/goalPole.urdf" />
    Start Gazebo with an empty world -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch"/>
        <arg name="verbose" value="true"/>
    <!--arg name="debug" value="true"/-->

    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find custom_models)/rviz_2.rviz" />

    
    <!-- Spawn a custom1 in Gazebo, taking the description from the
        parameter server -->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
    args="-param robot_description -urdf -model custom" />
    <node name="robot_state_publisher" pkg="robot_state_publisher"
        type="robot_state_publisher"/>
</launch>
